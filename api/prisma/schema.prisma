generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int    @id @default(autoincrement()) @db.UnsignedInt
  email    String @unique
  password String
  role     Int    @default(1) @db.UnsignedTinyInt
  status   Int    @default(1) @db.UnsignedTinyInt
}
